<Window x:Class="Sodoku.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sodoku"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800">
    <Grid RenderTransformOrigin="0.164,0.5" Margin="35,0,315,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="150" />
            <ColumnDefinition Width="150" />
            <ColumnDefinition Width="150" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="150" />
            <RowDefinition Height="150" />
            <RowDefinition Height="150" />
        </Grid.RowDefinitions>
        <Grid x:Name="Square1" Grid.Column="0" Grid.Row="0">
            <Border BorderBrush="Black" BorderThickness="4"/>
        </Grid>
        <Grid x:Name="Square2" Grid.Column="1" Grid.Row="0">
            <Border BorderBrush="Black" BorderThickness="4"/>
        </Grid>
        <Grid x:Name="Square3" Grid.Column="2" Grid.Row="0">
            <Border BorderBrush="Black" BorderThickness="4"/>
        </Grid>
        <Grid x:Name="Square4" Grid.Column="0" Grid.Row="1">
            <Border BorderBrush="Black" BorderThickness="4"/>
        </Grid>
        <Grid x:Name="Square5" Grid.Column="1" Grid.Row="1">
            <Border BorderBrush="Black" BorderThickness="4"/>
        </Grid>
        <Grid x:Name="Square6" Grid.Column="2" Grid.Row="1">
            <Border BorderBrush="Black" BorderThickness="4"/>
        </Grid>
        <Grid x:Name="Square7" Grid.Column="0" Grid.Row="2">
            <Border BorderBrush="Black" BorderThickness="4"/>
        </Grid>
        <Grid x:Name="Square8" Grid.Column="1" Grid.Row="2">
            <Border BorderBrush="Black" BorderThickness="4"/>
        </Grid>
        <Grid x:Name="Square9" Grid.Column="2" Grid.Row="2">
            <Border BorderBrush="Black" BorderThickness="4"/>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="50"/>
                    <ColumnDefinition Width="50"/>
                    <ColumnDefinition Width="50"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="50"/>
                    <RowDefinition Height="50"/>
                    <RowDefinition Height="50"/>
                </Grid.RowDefinitions>
            </Grid>
        </Grid>
    </Grid>
</Window>
